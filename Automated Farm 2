let x = CARROT



function BuildWheatFarm() {
agent.teleportToPlayer()
agent.setSlot(1)
    agent.destroy(DOWN)
    agent.setItem(WATER_BUCKET, 1, 1)
    agent.place(DOWN)
    agent.move(LEFT, 4)
    agent.move(BACK, 5)
    agent.move(BACK, 2)
    agent.setItem(CHEST, 1, 1)
    agent.place(LEFT)
    agent.move(FORWARD, 2)
for (let i = 0; i < 4; i++) {

for (let i = 0; i < 9; i++) {
agent.till(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)

for (let i = 0; i < 9; i++) {
agent.till(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
}

for (let i = 0; i < 9; i++) {
agent.till(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(LEFT, 4)
agent.move(BACK, 4)
player.say("Farm Built")

for (let i=0; i<10; i++){

agent.move(LEFT, 4)
agent.move(BACK, 5)
agent.setItem(x, 64, 1)
agent.setSlot(1)
for (let i = 0; i < 4; i++) {

for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)

for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
}


for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(LEFT, 4)
agent.move(BACK, 4)


for (let i=0; i<4; i++) {
agent.move(LEFT, 4)
agent.move(BACK, 5)
agent.setItem(BONE_MEAL, 64, 1)
agent.setSlot(1)
for (let i = 0; i < 4; i++) {

for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)

for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
}


for (let i = 0; i < 9; i++) {
agent.place(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(LEFT, 4)
agent.move(BACK, 4)

}



agent.move(LEFT, 4)
agent.move(BACK, 5)

for (let i = 0; i < 4; i++) {

for (let i = 0; i < 9; i++) {
agent.destroy(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)

for (let i = 0; i < 9; i++) {
agent.destroy(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
}


for (let i = 0; i < 9; i++) {
agent.destroy(FORWARD)
agent.move(FORWARD, 1)
}

agent.move(LEFT, 4)
agent.move(BACK, 4)

agent.move(LEFT, 4)
agent.move(BACK, 5)

for (let i = 0; i < 4; i++) {

for (let i = 0; i < 9; i++) {
agent.collectAll()
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Right)

for (let i = 0; i < 9; i++) {
agent.collectAll()
agent.move(FORWARD, 1)
}

agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
agent.move(FORWARD, 1)
agent.turn(TurnDirection.Left)
}


for (let i = 0; i < 9; i++) {
agent.collectAll()
agent.move(FORWARD, 1)
}

agent.move(LEFT, 4)
agent.move(BACK, 4)

agent.getItemCount(1)

agent.move(BACK, 2)
agent.interact(LEFT)

agent.move(FORWARD, 2)
}
}

function something () {
    agent.teleportToPlayer()
    agent.getItemCount(1)
}

player.onChat("run", something)


agent.drop(FORWARD, 1, 64)


player.onChat("wheatfarm", BuildWheatFarm )
